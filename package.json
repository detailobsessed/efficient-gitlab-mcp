{
  "name": "gitlab-mcp-server",
  "version": "3.0.0",
  "description": "Production-ready GitLab MCP Server with progressive disclosure pattern",
  "license": "MIT",
  "author": "",
  "type": "module",
  "main": "dist/index.js",
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/detailobsessed/efficient-gitlab-mcp.git"
  },
  "keywords": [
    "gitlab",
    "mcp",
    "model-context-protocol",
    "ai",
    "api"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "bun build src/server/index.ts --outdir dist --target node",
    "start": "bun dist/index.js",
    "dev": "bun run build && bun start",
    "test": "bun test",
    "check": "biome check --write",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.24.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "fetch-cookie": "^3.1.0",
    "form-data": "^4.0.0",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.6",
    "node-fetch": "^3.3.2",
    "open": "^10.2.0",
    "pkce-challenge": "^5.0.0",
    "socks-proxy-agent": "^8.0.5",
    "tough-cookie": "^5.1.2",
    "zod": "^3.24.2",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/bun": "^1.2.14",
    "@types/express": "^5.0.6",
    "@types/node": "^22.13.10"
  }
}
